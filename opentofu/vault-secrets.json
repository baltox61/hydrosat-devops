{
  "secrets": {
    "cluster": {
      "openweather-api-key": {
        "key": "${OPENWEATHER_API_KEY}"
      },
      "postgres": {
        "dagster-db-password": {
          "password": "${DAGSTER_DB_PASSWORD:-changeme-dagster-password}"
        }
      },
      "aws": {
        "s3-bucket-name": {
          "bucket": "${PRODUCTS_BUCKET:-dagster-weather-products}"
        }
      }
    },
    "dagster": {
      "pipeline-config": {
        "fetch_interval": "3600"
      },
      "coordinates": {
        "lat": "40.7128",
        "lon": "-74.0060"
      }
    },
    "api": {
      "rate-limits": {
        "max_requests": "100"
      }
    }
  },
  "vault_paths": {
    "dagster": [
      "secret/data/${CLUSTER_NAME}/openweather-api-key",
      "secret/data/${CLUSTER_NAME}/postgres/dagster-db-password",
      "secret/data/${CLUSTER_NAME}/aws/s3-bucket-name",
      "secret/data/dagster/pipeline-config",
      "secret/data/dagster/coordinates"
    ],
    "api": [
      "secret/data/${CLUSTER_NAME}/aws/s3-bucket-name",
      "secret/data/api/rate-limits"
    ]
  }
}
