{
  "title": "Dagster Pipeline",
  "tags": [
    "dagster",
    "pipeline",
    "orchestration"
  ],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "title": "Job Success Rate (24h)",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "100 * (sum(increase(dagster_job_success_total{component=\"user-deployments\"}[24h])) / sum(increase(dagster_job_total{component=\"user-deployments\"}[24h])))",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 80,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Total Jobs Run (24h)",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(dagster_job_total{component=\"user-deployments\"}[24h]))",
          "legendFormat": "Total Jobs",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Failed Jobs (1h)",
      "type": "stat",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(increase(dagster_job_failure_total{component=\"user-deployments\"}[1h]))",
          "legendFormat": "Failures",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 3,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value"
      }
    },
    {
      "title": "Avg Job Duration",
      "type": "stat",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "avg(dagster_run_duration_seconds{component=\"user-deployments\",job_name=\"weather_product_job\"})",
          "legendFormat": "Avg Duration",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 120,
                "color": "yellow"
              },
              {
                "value": 180,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Job Runs Over Time",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(rate(dagster_job_success_total{component=\"user-deployments\"}[5m])) by (job_name)",
          "legendFormat": "{{job_name}} (success)",
          "refId": "A"
        },
        {
          "expr": "sum(rate(dagster_job_failure_total{component=\"user-deployments\"}[5m])) by (job_name)",
          "legendFormat": "{{job_name}} (failed)",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Rate (jobs/sec)"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": {
        "shared": true
      },
      "seriesOverrides": [
        {
          "alias": "/.*failed.*/",
          "color": "#F2495C"
        },
        {
          "alias": "/.*success.*/",
          "color": "#73BF69"
        }
      ]
    },
    {
      "title": "Job Duration Trend",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "dagster_run_duration_seconds{component=\"user-deployments\",job_name=\"weather_product_job\"}",
          "legendFormat": "Weather Pipeline",
          "refId": "A"
        },
        {
          "expr": "dagster_run_duration_seconds{component=\"user-deployments\",job_name=\"demo_flaky_job\"}",
          "legendFormat": "Demo Job",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "max": true
      },
      "tooltip": {
        "shared": true
      }
    },
    {
      "title": "Demo Job Failure Rate",
      "type": "gauge",
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "100 * (rate(dagster_job_failure_total{component=\"user-deployments\",job_name=\"demo_flaky_job\"}[1h]) / rate(dagster_job_total{component=\"user-deployments\",job_name=\"demo_flaky_job\"}[1h]))",
          "legendFormat": "Failure Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 40,
                "color": "yellow"
              },
              {
                "value": 60,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      }
    },
    {
      "title": "Step Success Rate",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 10,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "100 * (sum(increase(dagster_step_success_total{component=\"user-deployments\"}[24h])) / sum(increase(dagster_step_total{component=\"user-deployments\"}[24h])))",
          "legendFormat": "Step Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 98,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Dagster Pod Memory (MB)",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 10,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{namespace=\"data\",pod=~\"dagster.*\"}) by (pod) / 1024 / 1024",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "mbytes",
          "label": "Memory"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "current": true
      },
      "tooltip": {
        "shared": true
      }
    },
    {
      "title": "Dagster Pod CPU",
      "type": "graph",
      "gridPos": {
        "x": 18,
        "y": 10,
        "w": 6,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"data\",pod=~\"dagster.*\"}[5m])) by (pod)",
          "legendFormat": "{{pod}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "CPU Cores"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "current": true
      },
      "tooltip": {
        "shared": true
      }
    },
    {
      "title": "Active Job Runs",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(dagster_jobs_running{component=\"user-deployments\"})",
          "legendFormat": "Running",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value_and_name"
      }
    },
    {
      "title": "Daemon Status",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 16,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "dagster_daemon_heartbeat{component=\"user-deployments\"}",
          "legendFormat": "Daemon",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                }
              }
            },
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value"
      }
    },
    {
      "title": "Jobs by Status (24h)",
      "type": "piechart",
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(increase(dagster_job_success_total{component=\"user-deployments\"}[24h]))",
          "legendFormat": "Success",
          "refId": "A"
        },
        {
          "expr": "sum(increase(dagster_job_failure_total{component=\"user-deployments\"}[24h]))",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "donut"
      }
    }
  ]
}
